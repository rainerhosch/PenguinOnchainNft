pragma solidity ^0.8.17;

interface IPenguinOnchain {
    struct Accessory {
        string trait_type;
        string trait_name;
        string bytePixel;
        uint256 sellingPrice;
        uint256 lastPrice;
        address owner;
        bool forSale;
    }

    struct SpecialTrait {
        string category;
        string networth;
    }

    struct Offer {
        address buyer;
        uint256 toTokenId;
        uint256 price;
        bool active;
    }

    function getNFTDetails(
        uint256 tokenId
    ) external view returns (Accessory[] memory, SpecialTrait memory);
}
